pipeline {
    agent any

    stages {
        stage('Build & Run Docker Image') {
            steps {
                script{
                    any.build("test-jenkins:0.1", ".")
                    any.image('test-jenkins:0.1').withRun('') { c->
                        sh "docker logs ${c.id}"
                    }
                }
            }
        }
    }
}